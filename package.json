{
  "name": "stack-exchange-markdown-retriever",
  "version": "1.1.6",
  "description": "Retrieves the markdown for a question or answer on a stack exchange site.",
  "main": "index.js",
  "scripts": {
    "test": "xo && nyc ava",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "travis-after-success": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/m4bwav/stack-exchange-markdown-retriever.git"
  },
  "keywords": [
    "Stack Exchange",
    "markdown",
    "question",
    "answer",
    "stackexchange",
    "stack overflow",
    "stackoverflow"
  ],
  "author": "Mark Rogers",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/m4bwav/stack-exchange-markdown-retriever/issues"
  },
  "homepage": "https://github.com/m4bwav/stack-exchange-markdown-retriever#readme",
  "devDependencies": {
    "ava": "^2.4.0",
    "codecov.io": "^0.1.6",
    "coveralls": "^3.0.9",
    "execa": "^3.4.0",
    "nyc": "^14.1.1",
    "snyk": "^1.251.2",
    "xo": "^0.25.3"
  },
  "xo": {
    "rules": {
      "xo/no-process-exit": "off",
      "linebreak-style": "off",
      "no-var": "off",
      "prefer-arrow-callback": "off",
      "func-names": "off",
      "no-multi-assign": "off",
      "capitalized-comments": "off",
      "indent": [
        "error",
        2
      ]
    }
  },
  "dependencies": {
    "commander": "^4.0.1",
    "request": "^2.88.0"
  }
}
